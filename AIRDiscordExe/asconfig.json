{
	"config": "air",
	"compilerOptions": {
		"source-path": [
			"src"
		],
		"library-path": [
			"extensions/DiscordWrapperANE.ane"
		],
		"output": "bin/Main.swf"
	},
	"mainClass": "Main",
    "application": "src/Main-app.xml",
    "airOptions": {
        "windows": {
            "target": "bundle",
            "output": "bin/Main",
            "signingOptions": {
                "storetype": "pkcs12",
                "keystore": "../../cert.pfx",
                "tsa": "http://zeitstempel.dfn.de"
            },
            "files": [

            ],
            "extdir": [
                "extensions"
            ]
        }
	}
}